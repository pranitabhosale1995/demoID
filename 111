if (
  status === 401 &&
  err.response?.data?.error === "TOKEN_EXPIRED"
) {
  console.log("Access token expired â†’ refreshing token");

  try {
    await refreshToken();   // ðŸ‘ˆ already exists
    return callApi(
      url,
      payload,
      method,
      responseType,
      contentType,
      extraConfig,
      returnDataOnly
    ); // ðŸ‘ˆ retry same API
  } catch (e) {
    console.error("Refresh token failed");
    navigate("/");
    throw e;
  }
}
